(cd ~/.xres && ./init)

disable bell beep
xset b off
dunst &
urxvtd &
clipmenud &
hsetroot &

# Keyboard config
# setxkbmap -layout "us,se" -option "grp:caps_switch"
# setxkbmap -layout "us" -variant colemak -option 'caps:escape' -option 'ctrl:ralt_rctrl'
setxkbmap -option 'caps:escape'
xmodmap ~/.config/colemak_dh_matrix.xmodmap

# set key repeat rate
xset r rate 200 35
