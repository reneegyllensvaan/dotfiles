#!/bin/sh
echo "disp-$RANDOM"
# if [ "$1" ]; then
  session_name=`tmux ls | grep -E '^disp-' | grep -v ' (attached)' | head -n 1 | grep -Eo '^[^:]*'`
  echo session name "$session_name"
  if [ "$session_name" != "" ]; then
    tmux attach-session -t "$session_name"
  else
    tmux new-session -s "disp-$RANDOM"
  fi
# else
#   tmux $@
# fi
