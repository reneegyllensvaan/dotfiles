{
  "docker.enable": true,

  "diagnostic.enable": true,
  "diagnostic.locationlistUpdate": true,
  "coc.preferences.useQuickfixForLocations": false,

  "coc.preferences.formatOnSaveFiletypes": ["rust", "python"],
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.source.buffer.ignoreGitignore": true,
  "tsserver.enableJavascript": true,
  "tsserver.log": "normal",
  "snippets.textmateSnippetsRoots": ["~/.config/textmateSnippets"],
  "eslint.enable": false,

  "languageserver": {
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"],
      "trace.server": "verbose"
    }
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",

  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.updates.prompt": false,
  "rust-analyzer.inlayHints.enable": true,

  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/Users/rgyllensva/backend/.venv-py3/bin/black",
  "python.formatting.blackArgs": ["-l100", "-tpy27"],
  "coc.preferences.willSaveHandlerTimeout": 1500,
  "coc.preferences.floatActions": false,
  "suggest.noselect": true,
  "snippets.ultisnips.pythonPrompt": false,
  "suggest.filterGraceful": false,
  "suggest.defaultSortMethod": "none"

}
