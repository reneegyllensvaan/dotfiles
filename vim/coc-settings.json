{
  "coc.preferences.formatOnSaveFiletypes": [],
  "python.formatting.blackPath": "/usr/local/bin/black",
  "clangd.semanticHighlighting": false,
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.source.buffer.ignoreGitignore": true,
  "tsserver.enableJavascript": true,
  "tsserver.log": "normal",
  // "tsserver.trace.server": "verbose",
  "snippets.textmateSnippetsRoots": ["~/.config/textmateSnippets"],
  "eslint.enable": false,
  "coc.preferences.enableFloatHighlight": false,
  "languageserver": {
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"],
      "trace.server": "verbose"
    }
  }
}
